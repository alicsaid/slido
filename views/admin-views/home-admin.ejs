    <script>
        var xValues = ["Jan", "Feb", "Mar", "Apr", "Maj", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec" ];
        var yValuesArrE = [];
        var barColors = ["red", "green", "blue", "orange", "black", "yellow", "pink", "indigo", "aqua", "purple", "grey", "darkseagreen"];

        <% for(var k = 0; k < yValuesE.length; k++){ %>
            yValuesArrE.push('<%= yValuesE[k] %>')
        <% } %>

        new Chart("myChart", {
            type: "bar",
            data: {
                labels: xValues,
                datasets: [{
                    backgroundColor: barColors,
                    data: yValuesArrE
                }]
            },
            options: {
                legend: {display: false},
                title: {
                    display: true,
                    text: "Events"
                },
                animation: {
                    duration: 3000,
                    easing: 'easeOutBounce'
                },
                scales: {
                    yAxes: [{
                        ticks: {
                            beginAtZero: true
                        }
                    }]
                }
            }
        });

        var xValuesArr = [];
        var yValuesArr = [];

        <% for(var i = 0; i < xValuesI.length; i++){ %>
            xValuesArr.push('<%= xValuesI[i] %>')
        <% } %>

        <% for(var j = 0; j < yValuesI.length; j++){ %>
            yValuesArr.push('<%= yValuesI[j] %>')
        <% } %>

        var barColors = [
            "#171111",
            "#00aba9",
            "#2b5797",
            "#e19f8c",
            "#1e7145",
            "#ff0000",
            "#e8e8e8",
            "#f84c5e",
            "#f5e1ec",
            "#d75da7",
            "#ffa400",
            "#2eff00",
        ];

        new Chart("myChart2", {
            type: "doughnut",
            data: {
                labels: xValuesArr,
                datasets: [{
                    backgroundColor: barColors,
                    data: yValuesArr
                }]
            },
            options: {
                title: {
                    display: true,
                    text: "Instructors"
                },
                animation: {
                    duration: 3000,
                    easing: 'easeInOutQuad'
                }
            }
        });
